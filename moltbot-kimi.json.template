{
  "meta": {
    "name": "moltbot-kimi",
    "version": "1.0.0",
    "description": "Moltbot powered by Kimi (Moonshot AI) - Affordable Chinese/English AI"
  },
  "models": {
    "providers": {
      "openai": {
        "apiKey": "${OPENAI_API_KEY}",
        "api": "openai",
        "baseURL": "${OPENAI_BASE_URL}",
        "models": [
          {
            "id": "moonshot-v1-8k",
            "name": "Moonshot v1 8K",
            "description": "Kimi's 8K context model - fast and affordable",
            "contextWindow": 8192,
            "maxTokens": 4096
          },
          {
            "id": "moonshot-v1-32k",
            "name": "Moonshot v1 32K",
            "description": "Kimi's 32K context model - good balance",
            "contextWindow": 32768,
            "maxTokens": 8192
          },
          {
            "id": "moonshot-v1-128k",
            "name": "Moonshot v1 128K",
            "description": "Kimi's 128K context model - large context support",
            "contextWindow": 128000,
            "maxTokens": 16384
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "openai/moonshot-v1-8k",
        "fallbacks": ["openai/moonshot-v1-32k"]
      },
      "models": {
        "openai/moonshot-v1-8k": {
          "alias": "Kimi 8K",
          "costOptimized": true
        },
        "openai/moonshot-v1-32k": {
          "alias": "Kimi 32K",
          "costOptimized": true
        },
        "openai/moonshot-v1-128k": {
          "alias": "Kimi 128K",
          "costOptimized": false
        }
      },
      "workspace": "/root/clawd"
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local"
  }
}